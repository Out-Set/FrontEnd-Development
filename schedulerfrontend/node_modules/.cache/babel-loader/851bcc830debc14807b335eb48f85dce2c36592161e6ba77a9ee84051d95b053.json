\n\r\n        <div class=\"setAndViewLogs\">\r\n\r\n            <!-- Set Expression -->\r\n            <div>\r\n                <h3 class=\"main\">Set Cron Value</h3>\r\n                <select v-model=\"cron\" style=\"width: 100px;\">\r\n                    <option v-for=\"cron in crons\" :key=\"cron\" :value=\"cron\">{{ cron }}</option>\r\n                </select>&nbsp;\r\n\r\n                <router-link to=\"/scheduleTask\">\r\n                    <button v-on:click=\"setCronToLS()\" class=\"mb-5\"\r\n                        :class=\"{ active: this.$route.path === '/scheduleTask' }\" aria-current=\"page\">Set\r\n                    </button>\r\n                </router-link>\r\n            </div>\r\n\r\n            <!-- Get Logs -->\r\n            <div>\r\n                <h3 class=\"main\">View Logs</h3>\r\n                <select v-model=\"cron\" style=\"width: 100px;\">\r\n                    <option v-for=\"cron in crons\" :key=\"cron\" :value=\"cron\">{{ cron }}</option>\r\n                </select>&nbsp;\r\n\r\n                <router-link :to=\"{ path: this.cron === 'cron-1' ? '/cron1Logs' : '/cron2Logs' }\">\r\n                    <button :class=\"{ active: this.$route.path === '/cronLogs' }\" aria-current=\"page\">View\r\n                    </button>\r\n                </router-link>\r\n            </div>\r\n\r\n            <!-- Stop prog Execution -->\r\n            <div>\r\n                <h3 class=\"main\">Stop and Start</h3>\r\n                <select v-model=\"cron\" style=\"width: 100px;\">\r\n                    <option v-for=\"cron in crons\" :key=\"cron\" :value=\"cron\">{{ cron }}</option>\r\n                </select>&nbsp;\r\n                <button v-on:click=\"stopCron(this.cron)\">Stop</button>&nbsp;\r\n                <button v-on:click=\"startCron(this.cron)\">Start</button>\r\n            </div>\r\n        </div>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    name: \"homePage\",\r\n    data() {\r\n        return {\r\n            name: \"Cron Dashboard\",\r\n            crons: ['cron-1', 'cron-2'],\r\n            cron: 'cron-1',\r\n\r\n            taskTypes: ['proc', 'api', 'method'],\r\n            taskType: 'proc'\r\n\r\n        }\r\n    },\r\n\r\n    methods: {\r\n        setCronToLS() {\r\n            localStorage.setItem('cronName', this.cron);\r\n        },\r\n\r\n        stopCron(cron) {\r\n            console.log('cron-name: ', cron);\r\n            axios.post('http://localhost:8082/tasks/stop?task=' + cron) // Dynamic-Cron\r\n                .then((response) => {\r\n                    console.log(\"Response form Backend: \", response);\r\n                    this.logs = response.data\r\n                })\r\n                .catch((error) => {\r\n                    // Handle the error\r\n                    console.log(\"Error Occured!\", error);\r\n                })\r\n        },\r\n\r\n        startCron(cron) {\r\n            console.log('cron-name: ', cron);\r\n            axios.post('http://localhost:8082/tasks/startAtInit?task=' + cron) // Dynamic-Cron\r\n                .then((response) => {\r\n                    console.log(\"Response form Backend: \", response);\r\n                    this.logs = response.data\r\n                })\r\n                .catch((error) => {\r\n                    // Handle the error\r\n                    console.log(\"Error Occured!\", error);\r\n                })\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n* {\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\n.master {\r\n    margin-top: 0px;\r\n    padding-top: 0px;\r\n    background: lightblue;\r\n    height: 660px;\r\n}\r\n\r\n.heading {\r\n    height: 40px;\r\n    background: lightcoral;\r\n    padding-top: 13px;\r\n    padding-left: 16px;\r\n}\r\n\r\n.main {\r\n    text-decoration: underline;\r\n    margin-bottom: 5px;\r\n}\r\n\r\n.setAndViewLogs {\r\n    display: grid;\r\n    grid-template-columns: auto auto auto;\r\n    margin-top: 50px;\r\n}\r\n\r\nselect {\r\n    padding: 3px 10px;\r\n}\r\n\r\nbutton {\r\n    border: none;\r\n    border-radius: 3px;\r\n    margin-bottom: 10px;\r\n    padding: 4px 20px;\r\n}\r\n\r\nbutton:hover {\r\n    background: #1397AA;\r\n    color: white;\r\n}\r\n</style>"],"mappings":"AAyDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHD,IAAI,EAAE,gBAAgB;MACtBE,KAAK,EAAE,CAAC,QAAQ,EAAE,QAAQ,CAAC;MAC3BC,IAAI,EAAE,QAAQ;MAEdC,SAAS,EAAE,CAAC,MAAM,EAAE,KAAK,EAAE,QAAQ,CAAC;MACpCC,QAAQ,EAAE;IAEd;EACJ,CAAC;EAEDC,OAAO,EAAE;IACLC,WAAWA,CAAA,EAAG;MACVC,YAAY,CAACC,OAAO,CAAC,UAAU,EAAE,IAAI,CAACN,IAAI,CAAC;IAC/C,CAAC;IAEDO,QAAQA,CAACP,IAAI,EAAE;MACXQ,OAAO,CAACC,GAAG,CAAC,aAAa,EAAET,IAAI,CAAC;MAChCJ,KAAK,CAACc,IAAI,CAAC,wCAAuC,GAAIV,IAAI,EAAE;MAAA,CACvDW,IAAI,CAAEC,QAAQ,IAAK;QAChBJ,OAAO,CAACC,GAAG,CAAC,yBAAyB,EAAEG,QAAQ,CAAC;QAChD,IAAI,CAACC,IAAG,GAAID,QAAQ,CAACd,IAAG;MAC5B,CAAC,EACAgB,KAAK,CAAEC,KAAK,IAAK;QACd;QACAP,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEM,KAAK,CAAC;MACxC,CAAC;IACT,CAAC;IAEDC,SAASA,CAAChB,IAAI,EAAE;MACZQ,OAAO,CAACC,GAAG,CAAC,aAAa,EAAET,IAAI,CAAC;MAChCJ,KAAK,CAACc,IAAI,CAAC,+CAA8C,GAAIV,IAAI,EAAE;MAAA,CAC9DW,IAAI,CAAEC,QAAQ,IAAK;QAChBJ,OAAO,CAACC,GAAG,CAAC,yBAAyB,EAAEG,QAAQ,CAAC;QAChD,IAAI,CAACC,IAAG,GAAID,QAAQ,CAACd,IAAG;MAC5B,CAAC,EACAgB,KAAK,CAAEC,KAAK,IAAK;QACd;QACAP,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEM,KAAK,CAAC;MACxC,CAAC;IACT;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}