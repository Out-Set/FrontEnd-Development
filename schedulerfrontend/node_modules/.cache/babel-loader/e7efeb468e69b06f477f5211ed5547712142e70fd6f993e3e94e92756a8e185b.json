<th scope=\"col\">DESCRIPTION</th>\r\n                            <th scope=\"col\">SERVICE TYPE</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    <tr v-for=\"data in (serviceIdentifiers)\" :key=\"data.jobName\">\r\n                        <td>{{ data.id }}</td>                        \r\n                        <td>\r\n                            <router-link class=\"no-decoration\" to=\"/auditDetails\" @click=\"saveServiceIdentifierId(data.id)\"> \r\n                                {{ data.name }}\r\n                            </router-link>\r\n                        </td>\r\n                        <td>{{ data.description }}</td>\r\n                        <td>{{ data.serviceType }}</td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n\r\n        <div class=\"footerDiv mt-1\">\r\n            <button @click=\"$router.back()\" class=\"btn btn-secondary btn-sm\">Back</button>\r\n        </div>\r\n    </div>\r\n</template>\r\n  \r\n<script>\r\nimport axios from 'axios';\r\nimport moment from 'moment';\r\nimport spinner from '../spinner.vue'\r\nimport sideBar from '../sideBar.vue'\r\n\r\nexport default {\r\n    name: \"serviceIdentifiers\",\r\n    components: {\r\n        spinner,\r\n        sideBar\r\n    },\r\n    data() {\r\n        return {\r\n            log: 'Service-Identifiers',\r\n            serviceIdentifiers: [],\r\n\r\n            loader: true,\r\n\r\n            ip: 'localhost'\r\n        }\r\n    },\r\n\r\n    mounted() {\r\n        axios.get('http://'+this.ip+':8081/BitsFlow-App/serviceIdentifiers')\r\n            .then((response) => {\r\n                console.log(\"Response form Backend: \", response);\r\n                this.serviceIdentifiers = response.data\r\n                this.loader = false\r\n            })\r\n            .catch((error) => {\r\n                // Handle the error\r\n                console.log(\"Error Occured!\", error);\r\n            })\r\n    },\r\n\r\n    methods: {\r\n        dateFormat(date) {\r\n            return moment(date).format('Do MMM YYYY, h:mm:ss a');\r\n        },\r\n\r\n        saveServiceIdentifierId(id) {\r\n            localStorage.setItem('serviceIdentifierId', id)\r\n        },\r\n    }\r\n};\r\n\r\n</script>\r\n  \r\n<style scoped>\r\n\r\n.allDataDiv {\r\n    padding: auto;\r\n    width: 90%;\r\n    height: 550px;\r\n    overflow: scroll;\r\n}\r\n\r\n.table {\r\n    /* width: 1270px; */\r\n}\r\n\r\nthead tr th {\r\n    background-color: lightskyblue;\r\n    font-size: 14px;\r\n}\r\n\r\ntbody tr td {\r\n    font-size: 12px;\r\n}\r\n\r\nthead {\r\n    position: sticky;\r\n    top: 0;\r\n}\r\n\r\n.heading {\r\n    height: 45px;\r\n    background: lightcoral;\r\n    padding-top: 10px;\r\n}\r\n\r\n.footerDiv {\r\n    width: 100%;\r\n    height: 30px;\r\n    display: flex;\r\n    justify-content: flex-end;\r\n    background-color: #c9c9c9;\r\n}\r\n\r\n.master {\r\n    margin-left: 5%;\r\n}\r\n\r\n@media (max-width: 767px) {\r\n    .masterDiv {\r\n        margin-left: 1%;\r\n    }\r\n}\r\n</style>"],"mappings":"AA0CA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,QAAQ;AAC3B,OAAOC,OAAM,MAAO,gBAAe;AACnC,OAAOC,OAAM,MAAO,gBAAe;AAEnC,eAAe;EACXC,IAAI,EAAE,oBAAoB;EAC1BC,UAAU,EAAE;IACRH,OAAO;IACPC;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,GAAG,EAAE,qBAAqB;MAC1BC,kBAAkB,EAAE,EAAE;MAEtBC,MAAM,EAAE,IAAI;MAEZC,EAAE,EAAE;IACR;EACJ,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACNX,KAAK,CAACY,GAAG,CAAC,SAAS,GAAC,IAAI,CAACF,EAAE,GAAC,uCAAuC,EAC9DG,IAAI,CAAEC,QAAQ,IAAK;MAChBC,OAAO,CAACR,GAAG,CAAC,yBAAyB,EAAEO,QAAQ,CAAC;MAChD,IAAI,CAACN,kBAAiB,GAAIM,QAAQ,CAACR,IAAG;MACtC,IAAI,CAACG,MAAK,GAAI,KAAI;IACtB,CAAC,EACAO,KAAK,CAAEC,KAAK,IAAK;MACd;MACAF,OAAO,CAACR,GAAG,CAAC,gBAAgB,EAAEU,KAAK,CAAC;IACxC,CAAC;EACT,CAAC;EAEDC,OAAO,EAAE;IACLC,UAAUA,CAACC,IAAI,EAAE;MACb,OAAOnB,MAAM,CAACmB,IAAI,CAAC,CAACC,MAAM,CAAC,wBAAwB,CAAC;IACxD,CAAC;IAEDC,uBAAuBA,CAACC,EAAE,EAAE;MACxBC,YAAY,CAACC,OAAO,CAAC,qBAA