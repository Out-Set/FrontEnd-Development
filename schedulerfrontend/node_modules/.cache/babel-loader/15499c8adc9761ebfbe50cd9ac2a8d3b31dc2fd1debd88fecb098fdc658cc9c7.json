{"ast":null,"code":"import axios from 'axios';\nimport moment from 'moment';\nimport spinner from '../Scheduler/spinner.vue';\nexport default {\n  name: \"auditDetails\",\n  components: {\n    spinner\n  },\n  data() {\n    return {\n      log: 'Audit-Details',\n      auditDetails: [],\n      date: '',\n      displayStyle: 'none',\n      popupData: '',\n      isPopupOpen: false,\n      dialogue_title: '',\n      requestString: '',\n      responseString: '',\n      intervalId: null,\n      loader: true,\n      ip: 'localhost'\n    };\n  },\n  mounted() {\n    let id = localStorage.getItem('serviceIdentifierId');\n    console.log(\"Id :: \", id);\n    axios.get('http://' + this.ip + ':8081/BitsFlow-App/currentDateLog?id=' + id).then(response => {\n      console.log(\"Response form Backend: \", response);\n      this.auditDetails = response.data;\n      this.loader = false;\n    }).catch(error => {\n      // Handle the error\n      console.log(\"Error Occured!\", error);\n    });\n  },\n  watch: {\n    date(selectedDate) {\n      console.log(`Selected Date :: ${selectedDate}`);\n      let id = localStorage.getItem('serviceIdentifierId');\n      axios.get('http://' + this.ip + ':8081/BitsFlow-App/selectedDateLog?date=' + this.date + '&id=' + id).then(response => {\n        console.log(\"Response form Backend: \", response);\n        this.auditDetails = response.data;\n        this.loader = false;\n      }).catch(error => {\n        // Handle the error\n        console.log(\"Error Occured!\", error);\n      });\n    }\n  },\n  methods: {\n    fetchRequestString(id) {\n      axios.get('http://' + this.ip + ':8081/BitsFlow-App/requestString?id=' + id).then(response => {\n        console.log(\"Response form Backend: \", response);\n        this.requestString = response.data;\n        this.loader = false;\n        return response.data;\n      }).catch(error => {\n        // Handle the error\n        console.log(\"Error Occured!\", error);\n      });\n    },\n    fetchResponseString(id) {\n      axios.get('http://' + this.ip + ':8081/BitsFlow-App/responseString?id=' + id).then(response => {\n        console.log(\"Response form Backend: \", response);\n        this.responseString = response.data;\n        this.loader = false;\n        setTimeout(() => {\n          this.popupData = \"Data fetched from API\";\n          this.isPopupOpen = true;\n        }, 1000);\n      }).catch(error => {\n        // Handle the error\n        console.log(\"Error Occured!\", error);\n      });\n    },\n    dateFormat(date) {\n      return moment(date).format('Do MMM YYYY, h:mm:ss a');\n    },\n    openPopup(title, id) {\n      if (title === 'REQUEST STRING') {\n        this.fetchRequestString(id);\n      } else {\n        this.fetchResponseString(id);\n      }\n\n      // this.popupData = data\n      // this.dialogue_title = title\n      // this.displayStyle = \"block\";\n      // if (this.loader == false) {\n      //     clearInterval(this.intervalId);\n      //     console.log(\"Interval stopped\");\n      // }\n    },\n\n    closePopup() {\n      this.displayStyle = \"none\";\n    },\n    fetch(title, id) {\n      let resp;\n      if (title === 'REQUEST STRING') {\n        this.loader = true;\n        resp = this.fetchRequestString(id);\n        resp = this.requestString;\n      } else {\n        this.loader = true;\n        this.fetchResponseString(id);\n        resp = this.responseString;\n      }\n      if (this.loader == true) {\n        this.intervalId = setInterval(this.openPopup(title, resp), 1000);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","moment","spinner","name","components","data","log","auditDetails","date","displayStyle","popupData","isPopupOpen","dialogue_title","requestString","responseString","intervalId","loader","ip","mounted","id","localStorage","getItem","console","get","then","response","catch","error","watch","selectedDate","methods","fetchRequestString","fetchResponseString","setTimeout","dateFormat","format","openPopup","title","closePopup","fetch","resp","setInterval"],"sources":["C:\\Users\\admin\\Documents\\Files\\FrontEnd-Development\\schedulerfrontend\\src\\components\\Integration\\requestResponseAuditDetails.vue"],"sourcesContent":["<template>\r\n    <spinner v-if=\"loader\"></spinner>\r\n\r\n    <div class=\"container-fluid\">\r\n        <div class=\"heading row\">\r\n            <h3>Audit Details</h3>\r\n        </div>\r\n\r\n        <div class=\"allDataDiv\" style=\"\">\r\n\r\n            <div class=\"filter\">\r\n                <!-- <label for=\"dateInput\">Select a date:</label> -->\r\n                <input type=\"date\" id=\"dateInput\" name=\"dateInput\" v-model=\"date\">\r\n            </div>\r\n\r\n            <table class=\"table table-hover\">\r\n                <thead>\r\n                    <tr>\r\n                        <th scope=\"col\">LOG ID</th>\r\n                        <th scope=\"col\">REQUEST STRING</th>\r\n                        <th scope=\"col\">REQUEST TIME STAMP</th>\r\n                        <th scope=\"col\">RESPONSE STRING</th>\r\n                        <th scope=\"col\">RESPONSE TIME STAMP</th>\r\n                        <th scope=\"col\">SOURCE SYSTEM IDENTIFIER</th>\r\n                        <th scope=\"col\">STATUS</th>\r\n                        <th scope=\"col\">TRANSACTION REFERENCE ID</th>\r\n                    </tr>\r\n                </thead>\r\n                <!-- //  <button v-on:click=\"isConditionTrue ? handleClickTrue : handleClickFalse\"></button> -->\r\n                <tbody>\r\n                    <tr v-for=\"data in (auditDetails)\" :key=\"data.jobName\">\r\n                        <td>{{ data[0] }}</td>\r\n                        <td>\r\n                            <button style=\"border: none;\" v-on:click=\"openPopup('REQUEST STRING', data[0])\">\r\n                                <i class=\"fa-regular fa-eye\"></i>\r\n                            </button>\r\n                        </td>\r\n                        <td>{{ dateFormat(data[7]) }}</td>\r\n                        <td>\r\n                            <button style=\"border: none;\" v-on:click=\"openPopup('RESPONSE STRING', data[0])\">\r\n                                <i class=\"fa-regular fa-eye\"></i>\r\n                            </button>\r\n                        </td>\r\n                        <td>{{ dateFormat(data[9]) }}</td>\r\n                        <td>{{ data[11] }}</td>\r\n                        <td>{{ data[12] }}</td>\r\n                        <td>{{ data[13] }}</td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n\r\n        <div class=\"modal\" tabindex=\"-1\" role=\"dialog\" v-bind:style=\"{ 'display': displayStyle }\">\r\n            <div class=\"modal-dialog\" role=\"document\">\r\n                <div class=\"modal-content\">\r\n                    <div class=\"modal-header\">\r\n                        <h4 class=\"modal-title\">{{ dialogue_title }}</h4>\r\n                        <button type=\"button\" class=\"btn-close\" v-on:click=\"closePopup()\" data-bs-dismiss=\"modal\"\r\n                            aria-label=\"Close\"> </button>\r\n                    </div>\r\n                    <div class=\"modal-body\" style=\"overflow: scroll;\">\r\n                        <p> {{ popupData }} </p>\r\n\r\n                    </div>\r\n                    <div class=\"modal-footer\">\r\n                        <button type=\"button\" class=\"btn btn-danger\" v-on:click=\"closePopup()\">\r\n                            Close\r\n                        </button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"footerDiv mt-1\">\r\n            <button @click=\"$router.back()\" class=\"btn btn-secondary btn-sm\">Back</button>\r\n        </div>\r\n    </div>\r\n</template>\r\n  \r\n<script>\r\nimport axios from 'axios';\r\nimport moment from 'moment';\r\nimport spinner from '../Scheduler/spinner.vue'\r\n\r\nexport default {\r\n    name: \"auditDetails\",\r\n    components: {\r\n        spinner\r\n    },\r\n    data() {\r\n        return {\r\n            log: 'Audit-Details',\r\n            auditDetails: [],\r\n\r\n            date: '',\r\n\r\n            displayStyle: 'none',\r\n            popupData: '',\r\n            isPopupOpen: false,\r\n            dialogue_title: '',\r\n\r\n            requestString: '',\r\n            responseString: '',\r\n\r\n            intervalId: null,\r\n\r\n            loader: true,\r\n\r\n            ip: 'localhost'\r\n        }\r\n    },\r\n\r\n    mounted() {\r\n        let id = localStorage.getItem('serviceIdentifierId')\r\n        console.log(\"Id :: \", id);\r\n        axios.get('http://' + this.ip + ':8081/BitsFlow-App/currentDateLog?id=' + id)\r\n            .then((response) => {\r\n                console.log(\"Response form Backend: \", response);\r\n                this.auditDetails = response.data\r\n                this.loader = false\r\n            })\r\n            .catch((error) => {\r\n                // Handle the error\r\n                console.log(\"Error Occured!\", error);\r\n            })\r\n    },\r\n\r\n    watch: {\r\n        date(selectedDate) {\r\n            console.log(`Selected Date :: ${selectedDate}`);\r\n            let id = localStorage.getItem('serviceIdentifierId')\r\n            axios.get('http://' + this.ip + ':8081/BitsFlow-App/selectedDateLog?date=' + this.date + '&id=' + id)\r\n                .then((response) => {\r\n                    console.log(\"Response form Backend: \", response);\r\n                    this.auditDetails = response.data\r\n                    this.loader = false\r\n                })\r\n                .catch((error) => {\r\n                    // Handle the error\r\n                    console.log(\"Error Occured!\", error);\r\n                })\r\n        },\r\n    },\r\n\r\n    methods: {\r\n        fetchRequestString(id) {\r\n            axios.get('http://' + this.ip + ':8081/BitsFlow-App/requestString?id=' + id)\r\n                .then((response) => {\r\n                    console.log(\"Response form Backend: \", response);\r\n                    this.requestString = response.data\r\n                    this.loader = false\r\n                    return response.data\r\n                })\r\n                .catch((error) => {\r\n                    // Handle the error\r\n                    console.log(\"Error Occured!\", error);\r\n                })\r\n        },\r\n\r\n        fetchResponseString(id) {\r\n            axios.get('http://' + this.ip + ':8081/BitsFlow-App/responseString?id=' + id)\r\n                .then((response) => {\r\n                    console.log(\"Response form Backend: \", response);\r\n                    this.responseString = response.data\r\n                    this.loader = false\r\n\r\n                    setTimeout(() => {\r\n                        this.popupData = \"Data fetched from API\";\r\n                        this.isPopupOpen = true;\r\n                    }, 1000);\r\n                })\r\n                .catch((error) => {\r\n                    // Handle the error\r\n                    console.log(\"Error Occured!\", error);\r\n                })\r\n\r\n        },\r\n\r\n        dateFormat(date) {\r\n            return moment(date).format('Do MMM YYYY, h:mm:ss a');\r\n        },\r\n\r\n        openPopup(title, id) {\r\n            if (title === 'REQUEST STRING') {\r\n                this.fetchRequestString(id)\r\n            } else {\r\n                this.fetchResponseString(id)\r\n            }\r\n\r\n\r\n            // this.popupData = data\r\n            // this.dialogue_title = title\r\n            // this.displayStyle = \"block\";\r\n            // if (this.loader == false) {\r\n            //     clearInterval(this.intervalId);\r\n            //     console.log(\"Interval stopped\");\r\n            // }\r\n        },\r\n        closePopup() {\r\n            this.displayStyle = \"none\";\r\n        },\r\n\r\n        fetch(title, id) {\r\n            let resp;\r\n            if (title === 'REQUEST STRING') {\r\n                this.loader = true\r\n                resp = this.fetchRequestString(id)\r\n                resp = this.requestString\r\n            } else {\r\n                this.loader = true\r\n                this.fetchResponseString(id)\r\n                resp = this.responseString\r\n            }\r\n\r\n            if (this.loader == true) {\r\n                this.intervalId = setInterval(this.openPopup(title, resp), 1000);\r\n            }\r\n        }\r\n    },\r\n};\r\n\r\n</script>\r\n  \r\n<style scoped>\r\n.filter {\r\n    text-align: right;\r\n    margin: 5px;\r\n    justify-content: right;\r\n    margin-right: 0px;\r\n}\r\n\r\n.allDataDiv {\r\n    padding: auto;\r\n    width: 100%;\r\n    height: 410px;\r\n    overflow: scroll;\r\n}\r\n\r\n.table {\r\n    width: 1250px;\r\n}\r\n\r\nthead tr th {\r\n    background-color: lightskyblue;\r\n    font-size: 14px;\r\n}\r\n\r\ntbody tr td {\r\n    font-size: 12px;\r\n}\r\n\r\nthead {\r\n    position: sticky;\r\n    top: 0;\r\n}\r\n\r\n.heading {\r\n    height: 45px;\r\n    background: lightcoral;\r\n    padding-top: 10px;\r\n}\r\n\r\n.footerDiv {\r\n    width: 100%;\r\n    height: 30px;\r\n    display: flex;\r\n    justify-content: flex-end;\r\n    background-color: #c9c9c9;\r\n}\r\n\r\n@media (max-width: 767px) {\r\n    .masterDiv {\r\n        margin-left: 1%;\r\n    }\r\n}\r\n</style>"],"mappings":"AAgFA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,QAAQ;AAC3B,OAAOC,OAAM,MAAO,0BAAyB;AAE7C,eAAe;EACXC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,GAAG,EAAE,eAAe;MACpBC,YAAY,EAAE,EAAE;MAEhBC,IAAI,EAAE,EAAE;MAERC,YAAY,EAAE,MAAM;MACpBC,SAAS,EAAE,EAAE;MACbC,WAAW,EAAE,KAAK;MAClBC,cAAc,EAAE,EAAE;MAElBC,aAAa,EAAE,EAAE;MACjBC,cAAc,EAAE,EAAE;MAElBC,UAAU,EAAE,IAAI;MAEhBC,MAAM,EAAE,IAAI;MAEZ