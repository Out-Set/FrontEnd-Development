          <!-- <td>\r\n                            <button style=\"border: none;\" v-on:click=\"openPopup('REQUEST STRING', data[0])\">\r\n                                <i class=\"fa-regular fa-eye\"></i>\r\n                            </button>\r\n                        </td> -->\r\n                        <td>\r\n                            <button style=\"border: none;\" v-on:click=\"fetchRequestString('REQUEST STRING', data[0])\">\r\n                                <i class=\"fa-regular fa-eye\"></i>\r\n                            </button>\r\n                        </td>\r\n                        <td>{{ dateFormat(data[7]) }}</td>\r\n                        <td>\r\n                            <button style=\"border: none;\" v-on:click=\"fetchResponseString('RESPONSE STRING', data[0])\">\r\n                                <i class=\"fa-regular fa-eye\"></i>\r\n                            </button>\r\n                        </td>\r\n                        <td>{{ dateFormat(data[9]) }}</td>\r\n                        <td>{{ data[11] }}</td>\r\n                        <td>{{ data[12] }}</td>\r\n                        <td>{{ data[13] }}</td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n\r\n        <div class=\"modal\" tabindex=\"-1\" role=\"dialog\" v-bind:style=\"{ 'display': displayStyle }\">\r\n            <div class=\"modal-dialog\" role=\"document\">\r\n                <div class=\"modal-content\">\r\n                    <div class=\"modal-header\">\r\n                        <h4 class=\"modal-title\">{{ dialogueTitle }}</h4>\r\n                        <button type=\"button\" class=\"btn-close\" v-on:click=\"closePopup()\" data-bs-dismiss=\"modal\"\r\n                            aria-label=\"Close\"> </button>\r\n                    </div>\r\n                    <div class=\"modal-body\" style=\"overflow: scroll;\">\r\n                        <p> {{ popupData }} </p>\r\n\r\n                    </div>\r\n                    <div class=\"modal-footer\">\r\n                        <button type=\"button\" class=\"btn btn-danger\" v-on:click=\"closePopup()\">\r\n                            Close\r\n                        </button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"footerDiv mt-1\">\r\n\r\n            <ul class=\"pagination pagination-sm\">\r\n                <li class=\"page-item\" :class=\"{ 'disabled': currentPage === 1 }\">\r\n                    <a class=\"page-link\" @click=\"changePage(currentPage - 1)\" aria-label=\"Previous\">\r\n                        <span aria-hidden=\"true\">&laquo;</span>\r\n                    </a>\r\n                </li>\r\n\r\n                <!-- <li class=\"page-item\" v-for=\"page in totalPages\" :key=\"page\" :class=\"{ 'active': page === currentPage }\">\r\n                    <a class=\"page-link\" @click=\"changePage(page)\">{{ page }}</a>\r\n                </li> -->\r\n\r\n                <li class=\"page-item\" :class=\"{ 'disabled': currentPage === totalPages }\">\r\n                    <a class=\"page-link\" @click=\"changePage(currentPage + 1)\" aria-label=\"Next\">\r\n                        <span aria-hidden=\"true\">&raquo;</span>\r\n                    </a>\r\n                </li>\r\n            </ul>\r\n\r\n            <button @click=\"$router.back()\" class=\"btn btn-secondary btn-sm\">Back</button>\r\n        </div>\r\n    </div>\r\n</template>\r\n  \r\n<script>\r\nimport axios from 'axios';\r\nimport moment from 'moment';\r\nimport spinner from '../Scheduler/spinner.vue'\r\n\r\nexport default {\r\n    name: \"auditDetails\",\r\n    components: {\r\n        spinner\r\n    },\r\n    data() {\r\n        return {\r\n            log: 'Audit-Details',\r\n            auditDetails: [],\r\n\r\n            date: '',\r\n\r\n            displayStyle: 'none',\r\n            popupData: '',\r\n            isPopupOpen: false,\r\n            dialogueTitle: '',\r\n\r\n            requestString: '',\r\n            responseString: '',\r\n\r\n            loader: true,\r\n\r\n            ip: 'localhost',\r\n\r\n            itemsPerPage: 10,\r\n            currentPage: 1,\r\n        }\r\n    },\r\n\r\n    mounted() {\r\n        // Current Date Logs\r\n        let id = localStorage.getItem('serviceIdentifierId')\r\n        console.log(\"Id :: \", id);\r\n        axios.get('http://' + this.ip + ':8081/BitsFlow-App/currentDateLog?id=' + id)\r\n            .then((response) => {\r\n                console.log(\"Response form Backend: \", response);\r\n                this.auditDetails = response.data\r\n                this.loader = false\r\n            })\r\n            .catch((error) => {\r\n                // Handle the error\r\n                console.log(\"Error Occured!\", error);\r\n            })\r\n    },\r\n\r\n    watch: {\r\n        // Selected Date Log\r\n        date(selectedDate) {\r\n            console.log(`Selected Date :: ${selectedDate}`);\r\n            let id = localStorage.getItem('serviceIdentifierId')\r\n            axios.get('http://' + this.ip + ':8081/BitsFlow-App/selectedDateLog?date=' + this.date + '&id=' + id)\r\n                .then((response) => {\r\n                    console.log(\"Response form Backend: \", response);\r\n                    this.auditDetails = response.data\r\n                    this.loader = false\r\n                })\r\n                .catch((error) => {\r\n                    // Handle the error\r\n                    console.log(\"Error Occured!\", error);\r\n                })\r\n        },\r\n    },\r\n\r\n    methods: {\r\n        fetchRequestString(title, id) {\r\n            this.loader = true\r\n            axios.get('http://' + this.ip + ':8081/BitsFlow-App/requestString?id=' + id)\r\n                .then((response) => {\r\n                    console.log(\"Response form Backend: \", response);\r\n\r\n                    // setTimeout(() => {\r\n                    //     this.popupData = response.data;\r\n                    //     this.isPopupOpen = true;\r\n                    // }, 1000);\r\n\r\n                    this.popupData = response.data;\r\n                    this.loader = false\r\n                    this.openPopup(title, id);\r\n                })\r\n                .catch((error) => {\r\n                    // Handle the error\r\n                    console.log(\"Error Occured!\", error);\r\n                })\r\n        },\r\n\r\n        fetchResponseString(title, id) {\r\n            this.loader = true\r\n            axios.get('http://' + this.ip + ':8081/BitsFlow-App/responseString?id=' + id)\r\n                .then((response) => {\r\n                    console.log(\"Response form Backend: \", response);\r\n\r\n                    // setTimeout(() => {\r\n                    //     this.popupData = response.data;\r\n                    //     this.isPopupOpen = true;\r\n                    // }, 1000);\r\n\r\n                    this.popupData = response.data;\r\n                    this.loader = false\r\n                    this.openPopup(title, id);\r\n                })\r\n                .catch((error) => {\r\n                    // Handle the error\r\n                    console.log(\"Error Occured!\", error);\r\n                })\r\n\r\n        },\r\n\r\n        dateFormat(date) {\r\n            return moment(date).format('Do MMM YYYY, h:mm:ss a');\r\n        },\r\n\r\n        openPopup(title) {\r\n            if (title === 'REQUEST STRING') {\r\n                this.dialogueTitle = title\r\n                // this.popupData = ''\r\n                // this.fetchRequestString(id)\r\n            } else {\r\n                this.dialogueTitle = title\r\n                // this.popupData = ''\r\n                // this.fetchResponseString(id)\r\n            }\r\n\r\n            this.displayStyle = \"block\";\r\n        },\r\n        closePopup() {\r\n            this.displayStyle = \"none\";\r\n        },\r\n\r\n        changePage(page) {\r\n            if (page >= 1 && page <= this.totalPages) {\r\n                this.currentPage = page;\r\n                this.fetchAuditDetails();\r\n            }\r\n        },\r\n    },\r\n\r\n    computed: {\r\n        totalPages() {\r\n            return Math.ceil(this.auditDetails.length / this.itemsPerPage);\r\n        },\r\n\r\n        paginatedItems() {\r\n            const startIndex = (this.currentPage - 1) * this.itemsPerPage;\r\n            const endIndex = startIndex + this.itemsPerPage;\r\n            return this.auditDetails.slice(startIndex, endIndex);\r\n        },\r\n    }\r\n};\r\n\r\n</script>\r\n  \r\n<style scoped>\r\n.filter {\r\n    text-align: right;\r\n    margin: 5px;\r\n    justify-content: right;\r\n    margin-right: 0px;\r\n}\r\n\r\n.allDataDiv {\r\n    padding: auto;\r\n    width: 100%;\r\n    height: 550px;\r\n    overflow: scroll;\r\n}\r\n\r\n.table {\r\n    width: 1270px;\r\n}\r\n\r\nthead tr th {\r\n    background-color: lightskyblue;\r\n    font-size: 14px;\r\n}\r\n\r\ntbody tr td {\r\n    font-size: 12px;\r\n}\r\n\r\nthead {\r\n    position: sticky;\r\n    top: 0;\r\n}\r\n\r\n.heading {\r\n    height: 45px;\r\n    background: lightcoral;\r\n    padding-top: 10px;\r\n}\r\n\r\n.footerDiv {\r\n    width: 100%;\r\n    height: 30px;\r\n    display: flex;\r\n    /* justify-content: flex-end; */\r\n    justify-content: space-between;\r\n    background-color: #c9c9c9;\r\n}\r\n\r\n/* .footerDiv button{\r\n    display: flex;\r\n    justify-content: flex-end;\r\n} */\r\n\r\n@media (max-width: 767px) {\r\n    .masterDiv {\r\n        margin-left: 1%;\r\n    }\r\n}\r\n</style>"],"mappings":"AAuGA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,QAAQ;AAC3B,OAAOC,OAAM,MAAO,0BAAyB;AAE7C,eAAe;EACXC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,GAAG,EAAE,eAAe;MACpBC,YAAY,EAAE,EAAE;MAEhBC,IAAI,EAAE,EAAE;MAERC,YAAY,EAAE,MAAM;MACpBC,SAAS,EAAE,EAAE;MACb