{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport moment from 'moment';\nimport spinner from './spinner.vue';\nexport default {\n  name: \"auditDetails\",\n  components: {\n    spinner\n  },\n  data() {\n    return {\n      log: 'Audit-Details',\n      auditDetails: [],\n      popupData: 'Hello ',\n      dialogue_buttons: {\n        buttonOne: {\n          enable: false,\n          title: ''\n        },\n        buttonTwo: {\n          enable: false,\n          title: ''\n        }\n      },\n      dialogue_actions: '',\n      dialogue_title: '',\n      loader: true,\n      filteredData: [],\n      ip: 'localhost'\n    };\n  },\n  mounted() {\n    axios.get('http://' + this.ip + ':8081/BitsFlow-App/allAuditDtlLog').then(response => {\n      console.log(\"Response form Backend: \", response);\n      this.auditDetails = response.data;\n      this.loader = false;\n    }).catch(error => {\n      // Handle the error\n      console.log(\"Error Occured!\", error);\n    });\n  },\n  methods: {\n    dateFormat(date) {\n      return moment(date).format('Do MMM YYYY, h:mm:ss a');\n    },\n    filter() {\n      this.filteredData = [];\n      for (const log of this.logs) {\n        if (log.taskName === this.taskName) {\n          this.filteredData.push(log);\n        }\n      }\n    },\n    openPopup(data, buttonOne, buttonTwo, title, action) {\n      this.popupData = data;\n      this.dialogue_buttons.buttonOne.enable = buttonOne.enable;\n      this.dialogue_buttons.buttonOne.title = buttonOne.title;\n      this.dialogue_buttons.buttonTwo.enable = buttonTwo.enable;\n      this.dialogue_buttons.buttonTwo.title = buttonTwo.title;\n      this.dialogue_actions = action;\n      this.dialogue_title = title;\n      this.displayStyle = \"block\";\n    },\n    closePopup() {\n      this.displayStyle = \"none\";\n    }\n  }\n};","map":{"version":3,"names":["axios","moment","spinner","name","components","data","log","auditDetails","popupData","dialogue_buttons","buttonOne","enable","title","buttonTwo","dialogue_actions","dialogue_title","loader","filteredData","ip","mounted","get","then","response","console","catch","error","methods","dateFormat","date","format","filter","logs","taskName","push","openPopup","action","displayStyle","closePopup"],"sources":["C:\\Users\\admin\\Documents\\Files\\FrontEnd-Development\\schedulerfrontend\\src\\components\\requestResponseAuditDetails.vue"],"sourcesContent":["<template>\r\n    <spinner v-if=\"loader\"></spinner>\r\n\r\n    <div class=\"container-fluid\">\r\n        <div class=\"heading row\">\r\n            <h3>Audit Details</h3>\r\n        </div>\r\n\r\n        <div class=\"allDataDiv\" style=\"\">\r\n            <table class=\"table table-hover\">\r\n                <thead>\r\n                    <tr>\r\n                        <th scope=\"col\">LOG ID</th>\r\n                        <th scope=\"col\">REQUEST STRING</th>\r\n                        <th scope=\"col\">REQUEST TIME STAMP</th>\r\n                        <th scope=\"col\">RESPONSE STRING</th>\r\n                        <th scope=\"col\">RESPONSE TIME STAMP</th>\r\n                        <th scope=\"col\">SERVICE IDENTIFIER ID</th>\r\n                        <th scope=\"col\">SOURCE SYSTEM IDENTIFIER</th>\r\n                        <th scope=\"col\">STATUS</th>\r\n                        <th scope=\"col\">TRANSACTION REFERENCE ID</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    <tr v-for=\"data in (auditDetails)\" :key=\"data.jobName\">\r\n                        <td>{{ data[0] }}</td>\r\n                        <td><button style=\"border: none;\"\r\n                                v-on:click=\"openPopup(data.jobAction, { enable: false, title: 'Close' }, { enable: true, title: 'Close' }, 'JOB ACTION', '')\">\r\n                                <i class=\"fa-regular fa-eye\"></i>\r\n                            </button>\r\n                        </td>\r\n                        <!-- <td>{{ data[6] }}</td>                         -->\r\n                        <td>{{ dateFormat(data[7]) }}</td>\r\n                        <td>{{ data[8] }}</td>\r\n                        <td>{{ dateFormat(data[9]) }}</td>\r\n                        <td>{{ data[10] }}</td>\r\n                        <td>{{ data[11] }}</td>\r\n                        <td>{{ data[12] }}</td>\r\n                        <td>{{ data[13] }}</td>\r\n\r\n                        <!-- <td>{{ data.id }}</td>\r\n                        <td>{{ data.requestString }}</td>\r\n                        <td>{{ data.responseString }}</td>\r\n                        <td>{{ dateFormat(data.requestTimeStamp) }}</td>\r\n                        <td>{{ dateFormat(data.responseTimeStamp) }}</td>\r\n                        <td>{{ data.sourceSystemIdentifier }}</td>\r\n                        <td>{{ data.serviceIdentifierId }}</td>\r\n                        <td>{{ data.transactionReferenceId }}</td>\r\n                        <td>{{ data.status }}</td> -->\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n\r\n        <div class=\"modal\" tabindex=\"-1\" role=\"dialog\" v-bind:style=\"{ 'display': displayStyle }\">\r\n            <div class=\"modal-dialog\" role=\"document\">\r\n                <div class=\"modal-content\">\r\n                    <div class=\"modal-header\">\r\n                        <h4 class=\"modal-title\">{{ dialogue_title }} {{ dialogue_actions }} RDM</h4>\r\n                        <button type=\"button\" class=\"btn-close\" v-on:click=\"closePopup()\" data-bs-dismiss=\"modal\"\r\n                            aria-label=\"Close\"></button>\r\n                    </div>\r\n                    <div class=\"modal-body\" style=\"overflow: scroll;\">\r\n                        <p v-if=\"!dialogue_buttons.buttonOne.enable\"> {{ popupData }} </p>\r\n                        <ul v-if=\"dialogue_buttons.buttonOne.enable\">\r\n                            <!-- {{ popupData }} -->\r\n                            <li v-for=\"item in popupData\" :key=\"item\">{{ item }}</li>\r\n                        </ul>\r\n\r\n                    </div>\r\n                    <div class=\"modal-footer\">\r\n                        <button type=\"button\" class=\"btn btn-danger\"\r\n                            v-on:click=\"enableAndDisableWithPopUp(dialogue_actions)\"\r\n                            v-if=\"dialogue_buttons.buttonOne.enable\">\r\n                            {{ dialogue_buttons.buttonOne.title }}\r\n                        </button>\r\n                        <button type=\"button\" class=\"btn btn-danger\" v-on:click=\"closePopup()\"\r\n                            v-if=\"dialogue_buttons.buttonTwo.enable\">\r\n                            {{ dialogue_buttons.buttonTwo.title }}\r\n                        </button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"footerDiv mt-1\">\r\n            <button @click=\"$router.back()\" class=\"btn btn-secondary btn-sm\">Back</button>\r\n        </div>\r\n    </div>\r\n</template>\r\n  \r\n<script>\r\nimport axios from 'axios';\r\nimport moment from 'moment';\r\nimport spinner from './spinner.vue'\r\n\r\nexport default {\r\n    name: \"auditDetails\",\r\n    components: {\r\n        spinner\r\n    },\r\n    data() {\r\n        return {\r\n            log: 'Audit-Details',\r\n            auditDetails: [],\r\n\r\n            popupData: 'Hello ',\r\n            dialogue_buttons: {\r\n                buttonOne: {\r\n                    enable: false,\r\n                    title: ''\r\n                },\r\n                buttonTwo: {\r\n                    enable: false,\r\n                    title: ''\r\n                }\r\n            },\r\n            dialogue_actions: '',\r\n            dialogue_title: '',\r\n\r\n            loader: true,\r\n\r\n            filteredData: [],\r\n\r\n            ip: 'localhost'\r\n        }\r\n    },\r\n\r\n    mounted() {\r\n        axios.get('http://' + this.ip + ':8081/BitsFlow-App/allAuditDtlLog')\r\n            .then((response) => {\r\n                console.log(\"Response form Backend: \", response);\r\n                this.auditDetails = response.data\r\n                this.loader = false\r\n            })\r\n            .catch((error) => {\r\n                // Handle the error\r\n                console.log(\"Error Occured!\", error);\r\n            })\r\n    },\r\n\r\n    methods: {\r\n        dateFormat(date) {\r\n            return moment(date).format('Do MMM YYYY, h:mm:ss a');\r\n        },\r\n\r\n        filter() {\r\n            this.filteredData = [];\r\n            for (const log of this.logs) {\r\n                if (log.taskName === this.taskName) {\r\n                    this.filteredData.push(log);\r\n                }\r\n            }\r\n        },\r\n\r\n        openPopup(data, buttonOne, buttonTwo, title, action) {\r\n            this.popupData = data;\r\n            this.dialogue_buttons.buttonOne.enable = buttonOne.enable;\r\n            this.dialogue_buttons.buttonOne.title = buttonOne.title;\r\n            this.dialogue_buttons.buttonTwo.enable = buttonTwo.enable;\r\n            this.dialogue_buttons.buttonTwo.title = buttonTwo.title;\r\n            this.dialogue_actions = action;\r\n            this.dialogue_title = title;\r\n            this.displayStyle = \"block\";\r\n        },\r\n        closePopup() {\r\n            this.displayStyle = \"none\";\r\n        },\r\n    }\r\n};\r\n\r\n</script>\r\n  \r\n<style scoped>\r\n.filter {\r\n    text-align: right;\r\n    margin: 5px;\r\n    justify-content: right;\r\n    margin-right: 0px;\r\n}\r\n\r\n.allDataDiv {\r\n    padding: auto;\r\n    width: 100%;\r\n    height: 410px;\r\n    overflow: scroll;\r\n}\r\n\r\n.table {\r\n    width: 1275px;\r\n}\r\n\r\nthead tr th {\r\n    background-color: lightskyblue;\r\n    font-size: 14px;\r\n}\r\n\r\ntbody tr td {\r\n    font-size: 12px;\r\n}\r\n\r\nthead {\r\n    position: sticky;\r\n    top: 0;\r\n}\r\n\r\n.heading {\r\n    height: 45px;\r\n    background: lightcoral;\r\n    padding-top: 10px;\r\n}\r\n\r\n.footerDiv {\r\n    width: 100%;\r\n    height: 30px;\r\n    display: flex;\r\n    justify-content: flex-end;\r\n    background-color: #c9c9c9;\r\n}\r\n\r\n@media (max-width: 767px) {\r\n    .masterDiv {\r\n        margin-left: 1%;\r\n    }\r\n}\r\n</style>"],"mappings":";AA4FA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,QAAQ;AAC3B,OAAOC,OAAM,MAAO,eAAc;AAElC,eAAe;EACXC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,GAAG,EAAE,eAAe;MACpBC,YAAY,EAAE,EAAE;MAEhBC,SAAS,EAAE,QAAQ;MACnBC,gBAAgB,EAAE;QACdC,SAAS,EAAE;UACPC,MAAM,EAAE,KAAK;UACbC,KAAK,EAAE;QACX,CAAC;QACDC,SAAS,EAAE;UACPF,MAAM,EAAE,KAAK;UACbC,KAAK,EAAE;QACX;MACJ,CAAC;MACDE,gBAAgB,EAAE,EAAE;MACpBC,cAAc,EAAE,EAAE;MAElBC,MAAM,EAAE,IAAI;MAEZC,YAAY,EAAE,EAAE;MAEhBC,EAAE,EAAE;IACR;EACJ,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACNnB,KAAK,CAACoB,GAAG,CAAC,SAAQ,GAAI,IAAI,CAACF,EAAC,GAAI,mCAAmC,EAC9DG,IAAI,CAAEC,QAAQ,IAAK;MAChBC,OAAO,CAACjB,GAAG,CAAC,yBAAyB,EAAEgB,QAAQ,CAAC;MAChD,IAAI,CAACf,YAAW,GAAIe,QAAQ,CAACjB,IAAG;MAChC,IAAI,CAACW,MAAK,GAAI,KAAI;IACtB,CAAC,EACAQ,KAAK,CAAEC,KAAK,IAAK;MACd;MACAF,OAAO,CAACjB,GAAG,CAAC,gBAAgB,EAAEmB,KAAK,CAAC;IACxC,CAAC;EACT,CAAC;EAEDC,OAAO,EAAE;IACLC,UAAUA,CAACC,IAAI,EAAE;MACb,OAAO3B,MAAM,CAAC2B,IAAI,CAAC,CAACC,MAAM,CAAC,wBAAwB,CAAC;IACxD,CAAC;IAEDC,MAAMA,CAAA,EAAG;MACL,IAAI,CAACb,YAAW,GAAI,EAAE;MACtB,KAAK,MAAMX,GAAE,IAAK,IAAI,CAACyB,IAAI,EAAE;QACzB,IAAIzB,GAAG,CAAC0B,QAAO,KAAM,IAAI,CAACA,QAAQ,EAAE;UAChC,IAAI,CAACf,YAAY,CAACgB,IAAI,CAAC3B,GAAG,CAAC;QAC/B;MACJ;IACJ,CAAC;IAED4B,SAASA,CAAC7B,IAAI,EAAEK,SAAS,EAAEG,SAAS,EAAED,KAAK,EAAEuB,MAAM,EAAE;MACjD,IAAI,CAAC3B,SAAQ,GAAIH,IAAI;MACrB,IAAI,CAACI,gBAAgB,CAACC,SAAS,CAACC,MAAK,GAAID,SAAS,CAACC,MAAM;MACzD,IAAI,CAACF,gBAAgB,CAACC,SAAS,CAACE,KAAI,GAAIF,SAAS,CAACE,KAAK;MACv